<settings>

    <profiles>

        <profile>
            <id>${env.ID_REPO_MVN}</id>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
            <repositories>
                <repository>
                    <id>${env.ID_REPO_MVN}</id>
                    <url>${env.URL_REPO_MVN}</url>
                </repository>
            </repositories>
        </profile>
        
    </profiles>

    <servers>
        <server>
            <id>${env.ID_REPO_MVN}</id>
            <username>aws</username>
            <password>${env.CODEARTIFACT_AUTH_TOKEN}</password>
        </server>
    </servers>
    
</settings>
